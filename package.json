{
  "name": "tamarin",
  "version": "1.0.28",
  "description": "Tamarin: extendable asynchronous wrapper around selenium-webdriver",
  "main": "index.js",
  "bin": "./bin/tamarin.js",
  "scripts": {
    "test": "node node_modules/mocha/bin/mocha",
    "coverage": "node node_modules/istanbul/lib/cli.js cover ./node_modules/mocha/bin/_mocha -- --ui bdd -R spec -t 5000",
    "snyk-protect": "snyk protect",
    "prepublish": "npm run snyk-protect"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ajaxscape/tamarin.git"
  },
  "keywords": [
    "tamarin",
    "cucumber",
    "bdd",
    "integration",
    "tests",
    "behaviour",
    "driven",
    "selenium",
    "webdriver",
    "webelement",
    "driver",
    "e2e",
    "cucumberjs",
    "testing",
    "world"
  ],
  "author": "Ben Surgison",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ajaxscape/tamarin/issues"
  },
  "standard": {
    "globals": [
      "describe",
      "it",
      "before",
      "beforeEach",
      "after",
      "afterEach",
      "expect"
    ]
  },
  "homepage": "https://github.com/ajaxscape/tamarin#readme",
  "devDependencies": {
    "chai": "^3.5.0",
    "chai-as-promised": "^6.0.0",
    "coveralls": "^2.11.14",
    "istanbul": "^0.4.5",
    "mocha": "^3.2.0",
    "mocha-lcov-reporter": "^1.2.0",
    "proxyquire": "^1.7.10",
    "sinon": "^1.17.6",
    "standard": "^8.6.0"
  },
  "dependencies": {
    "bluebird": "^3.4.6",
    "cli-spinner": "^0.2.5",
    "commander": "^2.9.0",
    "lodash": "^4.17.2",
    "selenium-webdriver": "^3.0.1",
    "snyk": "^1.232.0"
  },
  "snyk": true
}
